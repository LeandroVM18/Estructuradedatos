#include <iostream>

using namespace std;

int fibo(int n);
int main()
{
   int m;
   cout<<"cuanta cantidad desea"<<endl;
   cin>>m;
   for(int i=1;i<m;i++){
       cout<<fibo(i)<<" ";
       return 0;
   }
}

int fibo(int m)
{
    if (m==1){
        return m;
    }else{
        return fibo(m-1)+fibo(m-2);
    }
}
