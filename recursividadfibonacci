#include <iostream>

using namespace std;

int fibonacci(int);

int main()
{
    int n;
    cout<<"Escriba la cantidad de terminos"<<endl;
    cin>>n;
    for(int i;i<=n;i++)
    {
        cout<<fibonacci(i)<<" ";  
    }

    return 0;
}

int fibonacci(int n)
{
    if(n==1)
    {
        return n;
    }
    else
    {
        return (fibonacci(n-1)+fibonacci(n-2));
    }
}
